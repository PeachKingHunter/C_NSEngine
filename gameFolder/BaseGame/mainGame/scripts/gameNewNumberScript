.Script
//Next number
set lastResetFrame nbFrame;

// Reset all buttons's text
changeTextOf(button11, " ");
changeTextOf(button21, " ");
changeTextOf(button31, " ");
changeTextOf(button12, " ");
changeTextOf(button22, " ");
changeTextOf(button32, " ");
changeTextOf(button13, " ");
changeTextOf(button23, " ");
changeTextOf(button33, " ");

// Reset all buttons's image
changeImageOf(button11, " ");
changeImageOf(button21, " ");
changeImageOf(button31, " ");
changeImageOf(button12, " ");
changeImageOf(button22, " ");
changeImageOf(button32, " ");
changeImageOf(button13, " ");
changeImageOf(button23, " ");
changeImageOf(button33, " ");

// Change the button to click and write the number on it
randInto(numGamePos, 1, 9);
startScript gameButtonChangeNum numPos numGamePos val actuGameNum;

// The Red Block
startScript gameButtonResetRedBlock numPos redBlockGamePos;
if (actuGameNum > 9) { 
  randInto(redBlockGamePos, 1, 9);
  if (redBlockGamePos == numGamePos) {
    minus redBlockGamePos 1;
  }
  startScript gameButtonChangeRedBlock numPos redBlockGamePos;
}

// The RUBY
int rdm = 0;
randInto(rdm, 0, 99);
if (rdm < numberGetRubySpawnChance) { 
  randInto(rubyGamePos, 1, 9);
  if (rubyGamePos == numGamePos) {
    add rubyGamePos 1;
  }
  if (rubyGamePos == redBlockGamePos) {
    add rubyGamePos 1;
  }  
  if (rubyGamePos == numGamePos) {
    add rubyGamePos 1;
  }
  if (rubyGamePos == redBlockGamePos) {
    add rubyGamePos 1;
  }
  startScript gameButtonChangeImage numPos rubyGamePos val "ruby.png";
}
